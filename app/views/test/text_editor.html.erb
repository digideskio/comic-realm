<div class="container text-editor">
	<div class="toolbar" data-role="editor-toolbar" data-target="#editor">
		<div class="btn-group">
			<a class="btn" id="save-document" title data-original-title="Save"><i class="button fa fa-save"></i></a>
			<a class="btn" data-edit="undo" title data-original-title="Undo (Ctrl/Cmd+Z)"><i class="button fa fa-undo"></i></a>
			<a class="btn" data-edit="redo" title data-original-title="Redo (Ctrl/Cmd+Y)"><i class="button fa fa-repeat"></i></a>
		</div>
		<div class="separator"></div>
		<div class="btn-group">
			<a class="btn" data-edit="bold" title data-original-title="Bold (Ctrl/Cmd+B)"><i class="button fa fa-bold"></i></a>
			<a class="btn" data-edit="italic" title data-original-title="Italic (Ctrl/Cmd+I)"><i class="button fa fa-italic"></i></a>
			<a class="btn" data-edit="underline" title data-original-title="Underline (Ctrl/Cmd+U)"><i class="button fa fa-underline"></i></a>
			<a class="btn" data-edit="strikethrough" title data-original-title="Strikethrough"><i class="button fa fa-strikethrough"></i></a>
		</div>
		<div class="separator"></div>
		<div class="btn-group">
			<a class="btn btn-pressed" data-edit="justifyleft" title data-original-title="Align Left (Ctrl/Cmd+L)"><i class="button fa fa-align-left"></i></a>
			<a class="btn" data-edit="justifycenter" title data-original-title="Align Center (Ctrl/Cmd+E)"><i class="button fa fa-align-center"></i></a>
			<a class="btn" data-edit="justifyright" title data-original-title="Align Right (Ctrl/Cmd+R)"><i class="button fa fa-align-right"></i></a>
			<a class="btn" data-edit="justifyfull" title data-original-title="Align Jusify (Ctrl/Cmd+J)"><i class="button fa fa-align-justify"></i></a>
		</div>
		<div class="separator"></div>
		<div class="btn-group">
			<a class="btn" data-edit="insertunorderedlist" title data-original-title="Bullet List"><i class="button fa fa-list-ul"></i></a>
			<a class="btn" data-edit="insertorderedlist" title data-original-title="Number List"><i class="button fa fa-list-ol"></i></a>
			<!--<a class="btn" data-edit=""><i class="button fa fa-table"></i></a>-->
		</div>
	</div>
	<div class="script" id="editor"></div>
</div>